                                            <button class="btn btn-primary btn-block mt-4" id="resetBtn">
                                                Reset to
                                                Default
                                            </button>

                                            <a href="#" class="btn btn-danger btn-block mt-3" target="_blank">
                                                <i class="mdi mdi-basket mr-1"></i> Upgrade Now
                                            </a>

                                        </div>

                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
